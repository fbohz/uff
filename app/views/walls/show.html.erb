<h2> <%= @wall.location.city %> </h2>

<p> <%= @wall.description %> </p>

<p>Artist(s) Involved:</p>
<% @wall.artists.each do |a| %>
  <div><%= link_to a.name, artist_path(a) %></div>
<% end %>
<%= add_contr_details(@wall)%>

<p>Tags:</p>
<% @wall.tags.each do |t| %>
  <div>#<%= t.name %></div>
<% end %>

