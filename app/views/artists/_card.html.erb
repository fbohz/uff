      <div class="columns is-multiline is-1-mobile">
       
        <% model.each do |a|%>
        <div class="field column is-one-quarter">
          <div class="card">
            <div class="card-image">
              <figure class="image is-4by3">
                <img src="https://bulma.io/images/placeholders/1280x960.png" alt="Placeholder image">
              </figure>
            </div>
            <div class="card-content">
              <div class="media">
                <div class="media-left">
                  <figure class="image is-48x48">
                    <img src="https://bulma.io/images/placeholders/96x96.png" alt="Placeholder image">
                  </figure>
                </div>
                <div class="media-content">
                  <p class="title is-4"><%= link_to a.name, artist_path(a)%></p>
                  <p class="subtitle is-6"> <%= link_to "@#{a.instagram_username(a.instagram)}", a.instagram, target: :_blank unless !a.instagram %> </p>
                </div>
              </div>

              <div class="content">
                <p> <%= a.bio.truncate(100)%> </p>
                <%if a.tags.present?%>
                  <a> <%="##{a.tags.first.name }" %> </a>
                <%end%>
              </div>
            </div>
          </div>
        </div>
        <% end %>
      </div>